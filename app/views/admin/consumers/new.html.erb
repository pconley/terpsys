<h1>New Consumer</h1>

<%= simple_form_for(@consumer, :url => { :controller => "consumers", :action => "create" },) do |f| %>
  <%= render 'partials/model_errors', resource: @user %>
  <%= f.input :username %>
  <%= f.input :email %>  
  <%= f.input :password %>
  <%= f.input :last_name %>
  <%= f.input :first_name %>
  <%= f.button :submit, 'Save Consumer', class: 'button-create' %>
<% end %>

<%= link_to 'Back to List of Consumers', admin_consumers_path(:admin) %>