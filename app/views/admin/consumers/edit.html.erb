<h1>Edit Consumer</h1>

<%= simple_form_for(@consumer, :url => { :controller => "consumers", :action => "update" },) do |f| %>
  <%= render 'partials/model_errors', resource: @consumer %>
  <%= f.input :username %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :last_name %>
  <%= f.input :first_name %>
  <%= f.button :submit, class: 'button-create' %>
<% end %>

<%= link_to 'Show Consumer Details', admin_consumer_path(@consumer) %> |
<%= link_to 'Go to Consumers List', admin_consumers_path %>
