<h1>Edit Consumer</h1>

<%= simple_form_for(@user, :url => { :controller => "consumers", :action => "update" },) do |f| %>
  <%= render 'partials/model_errors', resource: @user %>
  <%= f.input :username %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :last_name %>
  <%= f.input :first_name %>
  <%= f.button :submit, class: 'button-create' %>
<% end %>

<%= link_to 'Show Details', consumer_path(@user) %> |
<%= link_to 'Go to Consumers List', consumers_path %>
