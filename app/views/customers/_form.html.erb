<%= simple_form_for(@consumer) do |f| %>

  <%= render 'partials/model_errors', resource: @consumer %>

  <%= f.simple_fields_for :user do |p| %>
  	<%= p.input :username %>
  	<%= p.input :email %>
  	<%= p.input :password %>
  <% end %>
  
  <%= f.input :company_name %>
  <%= f.input :requester_name %>
  <%= f.input :requester_email %>
  <%= f.input :billing_email %>
  <%= f.input :billing_rate, collection: [30,35,40], prompt: "Select your rate" %>

  <%= f.button :submit, class: 'button-create' %>

<% end %>
