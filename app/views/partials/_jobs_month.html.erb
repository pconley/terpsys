<h3>Jobs (Month View)</h3>
<div class='calendar job-calendar'>
<%= month_calendar events: events do |date,jobs| %>
	<%= date.strftime("%-d") %>
	<% jobs.take(5).each do |job| %>
		<% if view == :admin %>
    		<div><%= link_to "Job##{job.id}", admin_job_path(job) %></div>
    	<% end %>
  	<% end %>
	<% if jobs.count > 5 %>
		More...
	<% end %>
<% end %>
</div>
